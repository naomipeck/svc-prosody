@incollection{wichmann2012a,
  title = {Prosody in Context: {{The}} Effect of Sequential Relationships between Speaker Turns},
  shorttitle = {Prosody in Context},
  booktitle = {Prosody and {{Meaning}}},
  author = {Wichmann, Anne},
  editor = {Elordieta, Gorka and Prieto, Pilar},
  year = {2012},
  month = jan,
  pages = {329--348},
  publisher = {{DE GRUYTER}},
  address = {{Berlin, Boston}},
  doi = {10.1515/9783110261790.329},
  abstract = {People in conversation frequently accommodate their verbal and non-verbal behaviour to that of their interlocutor. It is observable in the matching of facial expression, posture and body movements.  It can also be observed in speech prosody, including accommodation of tempo, loudness and pitch. Accommodation, or convergence, is not yet well understood: some aspects appear to be an automatic response, but is also thought to be at least in part socially motivated.  This paper deals with accommodation in speech prosody, and focuses on the role played by accommodation of pitch in the generation of socio-pragmatic meaning, which can include the potential to reflect a degree of compliance, rapport or empathy with the interlocutor. Conveying compliance or lack of compliance is of particular importance in situations of conflict, and several studies are reported that highlight the strategic importance of vocal behaviour in controlling, minimising or escalating interpersonal conflict. The findings suggest that prosodic behaviour reflects the essentially co-operative nature of human communication and can be exploited as a social strategy. Furthermore, they suggest that attitudinal meaning is conveyed not only by the prosody of an individual utterance but by the sequential relationships between utterances.},
  isbn = {978-3-11-026179-0},
  langid = {english},
  file = {C\:\\Users\\npeck\\Zotero\\storage\\BR9M5ERK\\Wichmann - 2012 - Prosody in context The effect of sequential relat.pdf;C\:\\Users\\npeck\\Zotero\\storage\\N4PYCNW7\\Wichmann - 2012 - Prosody in context The effect of sequential relat.pdf}
}

@article{wichmannrama,
