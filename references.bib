@article{davidson2020,
  title = {The Versatility of Creaky Phonation: {{Segmental}}, Prosodic, and Sociolinguistic Uses in the World's Languages},
  shorttitle = {The Versatility of Creaky Phonation},
  author = {Davidson, Lisa},
  year = {2020},
  month = oct,
  journal = {WIREs Cognitive Science},
  issn = {1939-5078, 1939-5086},
  doi = {10/gh669r},
  abstract = {Creaky phonation (also known as creaky voice, vocal fry, laryngealization, or glottalization) is a voice quality that refers to shortened and thickened vocal folds that vibrate at a low and quasi-regular fundamental frequency with a long period of damping. Cross-linguistically, creaky phonation can span either short or long domains. When implemented on individual vowels or consonants (as in Zapotec or Montana Salish), it can signal phonemic contrast with other voice qualities, or it can be an additional acoustic cue to enhance other contrasts, such as tone (as in Mandarin or Cantonese). Another segmental use of creaky phonation in many languages is as a variant of glottal stop. Creaky phonation can also be implemented as a prosodic element that signals the end of a phrase (as in English or Mandarin), or indicates relinquishing a conversational turn (as in Finnish). It can also express meaning in a social interaction, such as irritation (in Vietnamese). Lastly, creaky phonation can be deployed as a sociolinguistic marker to establish identities, convey affect, or distinguish one speech group from another within the same language. In some social circumstances, such as the perception that young women use creaky phonation at greater rates than men do, it can be evaluated negatively by listeners. As creaky phonation can be combined with linguistic elements at various levels and is easily perceptible, it has taken on a remarkable number of roles in our linguistic repertoires.},
  langid = {english},
  file = {C\:\\Users\\npeck\\Zotero\\storage\\3XZ5PYJF\\Davidson - 2020 - The versatility of creaky phonation Segmental, pr.pdf;C\:\\Users\\npeck\\Zotero\\storage\\EQGEKH8W\\Davidson - 2020 - The versatility of creaky phonation Segmental, pr.pdf}
}

@article{davidson2021,
  title = {Effects of Word Position and Flanking Vowel on the Implementation of Glottal Stop: {{Evidence}} from {{Hawaiian}}},
  shorttitle = {Effects of Word Position and Flanking Vowel on the Implementation of Glottal Stop},
  author = {Davidson, Lisa},
  year = {2021},
