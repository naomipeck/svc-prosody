@article{goksunetal2010,
  title = {Trading {{Spaces}}: {{Carving}} up {{Events}} for {{Learning Language}}},
  shorttitle = {Trading {{Spaces}}},
  author = {G{\"o}ksun, Tilbe and {Hirsh-Pasek}, Kathy and Michnick Golinkoff, Roberta},
  year = {2010},
  month = jan,
  journal = {Perspectives on Psychological Science},
  volume = {5},
  number = {1},
  pages = {33--42},
  issn = {1745-6916, 1745-6924},
  doi = {10/bp7jnt},
  abstract = {Relational terms (e.g., verbs and prepositions) are the cornerstone of language development, bringing together two distinct fields: linguistic theory and infants' event processing. To acquire relational terms such as run, walk, in, and on, infants must first perceive and conceptualize components of dynamic events such as containment\textendash support, path\textendash manner, source\textendash goal, and figure\textendash ground. Infants must then uncover how the particular language they are learning encodes these constructs. This review addresses the interaction of language learning with infants' conceptualization of these nonlinguistic spatial event components. We present the thesis that infants start with language-general nonlinguistic constructs that are gradually refined and tuned to the requirements of their native language. In effect, infants are trading spaces, maintaining their sensitivity to some relational distinctions while dampening other distinctions, depending on how their native language expresses these constructs.},
  langid = {english},
  file = {C\:\\Users\\npeck\\Zotero\\storage\\PCKFZQUJ\\Göksun et al. - 2010 - Trading Spaces Carving up Events for Learning Lan.pdf;C\:\\Users\\npeck\\Zotero\\storage\\WEE2QFM6\\Göksun et al. - 2010 - Trading Spaces Carving up Events for Learning Lan.pdf}
}

