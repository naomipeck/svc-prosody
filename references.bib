@article{bohnemeyerbrown2007,
  title = {Standing Divided: Dispositionals and Locative Predications in Two {{Mayan}} Languages},
  shorttitle = {Standing Divided},
  author = {Bohnemeyer, J{\"u}rgen and Brown, Penelope},
  year = {2007},
  month = jan,
  journal = {Linguistics},
