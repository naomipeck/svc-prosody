@article{scottetal,
  title = {A {{Preliminary Report}} on {{Some Linguistic}} and {{Sociolinguistic Features}} of {{Papuan Malay}}},
  author = {Scott, G R and Kim, H and Rumaropen, B E W and Scott, E L and Nussy, C G and Yumbi, A C M and Cochran, R C},
  pages = {131},
  abstract = {A series of rapid appraisal surveys were conducted during 2007 at various locations in Papua, Indonesia as a means of providing an overview of some key linguistic and sociolinguistic features of the regional Malay variety/varieties (denoted herein as Papuan Malay) spoken on or near the coastal regions of Papua. To accomplish the survey, native speakers of Papuan Malay (PM) conducted informal group interviews regarding sociolinguistic features of PM via focus groups held in both urban and rural locations in each of five major kabupatens in the coastal region (Jayapura, Sorong, FakFak, Merauke, Mimika). All group interview participants were asked to complete an individual questionnaire regarding personal language use and family language use history. In addition, recorded texts and targeted elicitations were collected from a sub-group of the group interview participants. Finally, choice of language used and speaker/listener demographics were recorded for conversations observed (N = 989) in a variety of domains. Background research indicates that PM is likely historically derived from other eastern regional Malays (e.g., Ambonese Malay, North Moluccan Malay, etc.) which have their roots in a bazaar Malay used as a trade language. Sociolinguistically, PM vitality was observed to be strong and its use widespread. Shift from use of vernacular languages to PM appears to have been strong over the last couple of generations, particularly for those raised in Papua's urban centers. Papuan Malay is used in all domains except in some formal situations in both the educational (e.g., delivering formal instruction) and religious (e.g., preaching, leading corporate prayer) domains. Language attitudes towards PM are quite positive among indigenous Papuans and desire for the development of written materials in PM was expressed. Linguistically, preliminary analysis of the data corroborates previous conclusions in the literature that PM exhibits some key differences with standard Indonesian in the areas of phonology, syntax, morphology, and lexicon. Discourse features were also observed to differ. In conculsion, we believe there is strong justification for the establishment of an official language development program in Papuan Malay.},
  langid = {english},
  file = {C\:\\Users\\npeck\\Zotero\\storage\\EDH68T4E\\Scott et al. - A Preliminary Report on Some Linguistic and Sociol.pdf}
}

