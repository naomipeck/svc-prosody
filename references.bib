@article{zhaoetal2013,
  title = {Assumptions behind {{Intercoder Reliability Indices}}},
  author = {Zhao, Xinshu and Liu, Jun S. and Deng, Ke},
  year = {2013},
  month = jan,
  journal = {Annals of the International Communication Association},
