@article{wraygrace2007,
  title = {The Consequences of Talking to Strangers: {{Evolutionary}} Corollaries of Socio-Cultural Influences on Linguistic Form},
  shorttitle = {The Consequences of Talking to Strangers},
  author = {Wray, Alison and Grace, George W.},
  year = {2007},
  month = mar,
  journal = {Lingua},
  volume = {117},
  number = {3},
  pages = {543--578},
  issn = {00243841},
  doi = {10/d9mz43},
  abstract = {We explore the proposal that the linguistic forms and structures employed by our earliest language-using ancestors might have been significantly different from those observed in the languages we are most familiar with today, not because of a biological difference between them and us, but because the communicative context in which they operated was fundamentally different from that of most modern humans. Languages that are used predominantly for esoteric (intra-group) communication tend to have features that are semantically and grammatically `complex', while those used also (or even exclusively) for exoteric (intergroup) communication become `simplified' towards rule-based regularity and semantic transparency. Drawing on a range of contemporary data, we propose a psycholinguistic explanation for why esotericity would promote such complexity, and argue that this is the natural default setting for human language. This being so, it should be taken into account when modelling the evolution of language, for some of the features that are normally viewed as fundamental \textendash{} including the notion of fully developed underlying rule-based systematicity \textendash{} may, in fact, be cultural add-ons.},
  langid = {english},
  file = {C\:\\Users\\npeck\\Zotero\\storage\\7HTTT259\\Wray and Grace - 2007 - The consequences of talking to strangers Evolutio.pdf;C\:\\Users\\npeck\\Zotero\\storage\\MQVR4J8Y\\Wray and Grace - 2007 - The consequences of talking to strangers Evolutio.pdf}
}

