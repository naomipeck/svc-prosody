@article{namiki2010,
  title = {Morphological Variation in {{Japanese}} Compounds: {{The}} Case of Hoodai and the Notion of ``Compound-Specific Submeaning''},
  shorttitle = {Morphological Variation in {{Japanese}} Compounds},
  author = {Namiki, Takayasu},
