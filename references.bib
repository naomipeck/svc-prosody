  author = {San, Nay and Bartelds, Martijn and {\`O}g{\'u}nr{\textbackslash}d{\`e}m{\'i}, Tol{\'u}l{\textbackslash}d\{o\}p{\textbackslash}d{\'e} and Mount, Alison and Thompson, Ruben and Higgins, Michael and Barker, Roy and Simpson, Jane and Jurafsky, Dan},
  year = {2022},
