@book{schmidtke-bodeetal2019,
  title = {Explanation in Typology: {{Diachronic}} Sources, Functional Motivations and the Nature of the Evidence},
  shorttitle = {Explanation in Typology},
  author = {{Schmidtke-Bode}, Karsten and Levshina, Natalia and Michaelis, Susanne Maria and Ser{\v z}ant, Ilja A.},
  year = {2019},
  month = mar,
  publisher = {{Zenodo}},
  doi = {10.5281/ZENODO.2583788},
  abstract = {This volume provides an up-to-date discussion of a foundational issue that has recently{$<$}br{$>$} taken centre stage in linguistic typology and which is relevant to the language sciences{$<$}br{$>$} more generally: To what extent can cross-linguistic generalizations, i.e. statistical universals of linguistic structure, be explained by the diachronic sources of these structures?{$<$}br{$>$} Everyone agrees that typological distributions are the result of complex histories, as ``languages evolve into the variation states to which synchronic universals pertain'' (Hawkins 1988). However, an increasingly popular line of argumentation holds that many,{$<$}br{$>$} perhaps most, typological regularities are long-term reflections of their diachronic sources, rather than being `target-driven' by overarching functional-adaptive motivations.{$<$}br{$>$} On this view, recurrent pathways of reanalysis and grammaticalization can lead to uniform synchronic results, obviating the need to postulate global forces like ambiguity{$<$}br{$>$} avoidance, processing efficiency or iconicity, especially if there is no evidence for such{$<$}br{$>$} motivations in the genesis of the respective constructions. On the other hand, the recent{$<$}br{$>$} typological literature is equally ripe with talk of `complex adaptive systems', `attractor{$<$}br{$>$} states' and `cross-linguistic convergence'. One may wonder, therefore, how much room{$<$}br{$>$} is left for traditional functional-adaptive forces and how exactly they influence the diachronic trajectories that shape universal distributions. The papers in the present volume{$<$}br{$>$} are intended to provide an accessible introduction to this debate. Covering theoretical,{$<$}br{$>$} methodological and empirical facets of the issue at hand, they represent current ways{$<$}br{$>$} of thinking about the role of diachronic sources in explaining grammatical universals,{$<$}br{$>$} articulated by seasoned and budding linguists alike.},
  copyright = {Creative Commons Attribution 4.0 International, Open Access},
  langid = {english},
  file = {C\:\\Users\\npeck\\Zotero\\storage\\PAV397KQ\\Schmidtke-Bode et al. - 2019 - Explanation in typology Diachronic sources, funct.pdf}
}

