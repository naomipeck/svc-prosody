@article{quakenbush,
  title = {Some {{Agutaynen Grammatical Details}}:},
  author = {Quakenbush, Steve},
  pages = {29},
  abstract = {This paper outlines the basic details of Agutaynen personal pronouns, nominal markers, and tense/aspect/mode verbal affixation. A typologically interesting phenomenon occurs in a sequence of two Agutaynen pronouns in a Patient-Voice (transitive) clause, whereby Patients consisting of a first- or second-person pronoun occur first, violating the ``expected'' order of Actor-first (in Genitive form) and replacing it with Actor-last (in Oblique form). Agutaynen has three basic types of nominal markers for personal noun phrases: Nominative, Genitive and Oblique. Markers for common noun phrases show variation in each type, however. A definite/indefinite distinction in the ``Genitive'' leads to the analysis of Agutaynen tang as a Definite marker. Further variation in the ``Nominative'' and ``Oblique'' forms are also considered. Agutaynen makes a three-way, tense/aspect distinction--Perfective, Imperfective and Contemplated\textemdash easily demonstrated through the mag- set of Actor-voice (intransitive) prefixes. Two aspectual particles (da and pa) and one modal particle (din) are also considered.},
  langid = {english},
