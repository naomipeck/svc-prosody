@article{haig2018,
  title = {The Grammaticalization of Object Pronouns: {{Why}} Differential Object Indexing Is an Attractor State},
  shorttitle = {The Grammaticalization of Object Pronouns},
  author = {Haig, Geoffrey},
  year = {2018},
